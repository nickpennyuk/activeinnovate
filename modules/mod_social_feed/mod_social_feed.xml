<?xml version="1.0" encoding="utf-8"?><extension type="module" version="3.0" method="install" client="site">	<name>Social Feed</name>	<creationDate>November 2014</creationDate>	<author>Sean Jermey</author>	<authorEmail>seanjermey@bigwavemedia.co.uk</authorEmail>	<authorURL>http://www.bigwavemedia.co.uk</authorURL>	<copyright>Copyright (C) Big WaveM edia Ltd</copyright>	<license>GNU GPL</license>	<version>2.0.0</version>	<description>This will display twitter and facebook API feeds.</description>	<files>		<filename module="mod_social_feed">mod_social_feed.php</filename>		<filename>helper.php</filename>		<filename>index.html</filename>		<folder>assets</folder>		<folder>tmpl</folder>		<folder>helpers</folder>	</files>	<config>		<fields name="params">			<fieldset name="basic">				<field name="twitter"					type="Repeatable"					icon="list"					label="Twitter"					default='{"name":["",""]}'>					<fieldset hidden="true" name="twitter_modal" repeat="true">						<field name="name" label="Screename" type="text" />					</fieldset>				</field>				<field name="facebook"					type="Repeatable"					icon="list"					label="Facebook"					default='{"name":["",""]}'>					<fieldset hidden="true" name="facebook_modal" repeat="true">						<field name="name" label="Screename" type="text" />					</fieldset>				</field>												<field name="refresh_time" type="integer" label="Refresh Time ( mins )" description="" first="0" last="15" step="1" />																				</fieldset>			<fieldset name="advanced">				<field					name="layout"					type="modulelayout"					label="JFIELD_ALT_LAYOUT_LABEL"					description="JFIELD_ALT_MODULE_LAYOUT_DESC" />				<field					name="moduleclass_sfx"					type="textarea" rows="3"					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />				<field					name="cache"					type="list"					default="1"					label="COM_MODULES_FIELD_CACHING_LABEL"					description="COM_MODULES_FIELD_CACHING_DESC"				>					<option value="1">JGLOBAL_USE_GLOBAL</option>					<option value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>				</field>				<field					name="cache_time"					type="text"					default="900"					label="COM_MODULES_FIELD_CACHE_TIME_LABEL"					description="COM_MODULES_FIELD_CACHE_TIME_DESC" />				<field					name="cachemode"					type="hidden"					default="static"				>					<option value="static"></option>				</field>			</fieldset>		</fields>	</config></extension>